.r1
  color red
.r-li-bg
  background-color #fff
.c2,.c3
  background #f0f0f0
  .c2-ul
    min-height 230px
    li
      height 30px
      line-height 30px
      text-align center
  .c2-bom
    display flex
    height 30px
    padding 10px 0 10px 0
    line-height 30px
    .bom-left
      flex 4.71
      .l-box
        background #fff
        height 30px
        width 43%
        margin-left 56%
        text-align center
    .bom-mid
      flex 1.28
      height 30px
      position relative
      &:after
        content ""
        display block
        position absolute
        height 1px
        width 20px
        background #b4b4b4
        top 50%
        left 50%
        transform translate(-50%,-50%)
