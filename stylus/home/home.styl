        margin-left 12px
      .r-3
        flex 1.67
        height 30px
.sec
  margin-bottom 4px
  display flex
  height 113px
  border-bottom 1px solid #d6d7dc
  background #fff
  .sec-left
    box-sizing border-box
    padding 10px
    flex 3.56
    height 113px
    .l-img
      width 100%
      height 100%
      background url("../../APP切图/首页/content_img_list@2x.png") no-repeat center
      background-size 100%
  .sec-right
    box-sizing border-box
    flex 6.44
    height 113px
    padding 10px
    font-size 0.9em
    .h1
      height 26px
      line-height 26px
      font-size 1em
      background-size 8%
    .h11
      background url("../../APP切图/首页/collect_icon_press@2x.png") no-repeat right 20px center
    .h12
      background url("../../APP切图/首页/collect_icon_nor@2x.png") no-repeat right 20px center
    .h2
      height 14px
      line-height 14px
      font-size .9rem
    .h3
      height 16px
      padding 5px 0 5px 0
      .h3-box
        height 16px
        border 1px solid red
        border-radius 4px
        text-align center
        width 60px
        line-height 16px
        font-size .9em
        color #ea5404
    .h4
      height 15px
      line-height 15px
      display flex
      font-size .9em
      .h4-1
        flex 6.6
        em
          font-style normal
          color red
      .h4-2
        flex 4.4

