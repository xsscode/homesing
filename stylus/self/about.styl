*
 outline none
 font-family Droid Sans Fallback
@import "../head.styl"

body
  background-color #efeff4
  padding 0
  margin 0
.header
  background #fff
  .kk
    color #323232

.pic1
  height 125px
  background url("../../APP切图/关于/content_img_logo@2x.png") no-repeat center bottom 10px
  background-size 74px
.pic2
  height 25px
  background url("../../APP切图/关于/content_text_logo@2x.png") no-repeat center bottom 0px
  background-size 71px 19px

.sec2
  &:after
    content ""
    position absolute
    display block
    width 12px
    height 2px
    background #b4b4b4
    transform rotate(45deg)
    top 21px
    right 12px
  &:before
    content ""
    position absolute
    display block
    width 12px
    height 2px
    background #b4b4b4
    transform rotate(-45deg)
    top 28px
    right 12px
  .num
    position absolute
    width 5rem
    height 50px
    right 24px
    top 0
    line-height 50px
    font-size .9rem
    color #b4b4b4b4

.set-sec
  height 50px
  background #fff
  box-sizing border-box
  padding 18px 12px 16px 12px
  line-height 16px
  font-size 1rem
  color #323232
  position relative
  border-bottom #d6d7dc
  margin-top 10px