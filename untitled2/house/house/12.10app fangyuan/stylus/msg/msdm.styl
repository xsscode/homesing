



@import("./msghead.styl")
.sec
  margin 12px 12px 0 12px
  padding 0px 12px 12px 12px
  background #fff
  border-radius 5px
  box-shadow 0 0 5px 1px #e0e0ea
  min-height 110px
  .secheader
     height 43px
     line-height 43px
     font-size 1em
     display flex
     .head-txt
       flex 1
     .head-btn
       line-height 20px
       width 46px
       height 20px
       margin 12px
       font-size 0.9em

       text-align center
       border-radius 4px
  .secmid
     line-height 24px
     font-size 0.9em
  .secfooter
     height 35px
     display flex
    .time
      flex 1
      text-align right
      font-style normal
      padding 10px 12px 0 12px
      line-height 25px
      color #7c7c7d
    .footn
       width 25%
       height 25px
       border-radius 6px
       margin 10px auto 0px auto
       line-height 25px
       font-size 0.9em
       text-align center
       background #efeff4
       color #7c7c7d
 .c1
    background #15b755
 .c2
    background #0193e6
 .c3
    background #97b715
 .c4
    background #ea5404
 @import('./msgfooter.styl')








